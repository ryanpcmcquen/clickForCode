{
    "manifest_version": 2,
    "name": "clickForCode",
    "description": "Click to add a <pre> tag inside the HTML <body>.",
    "version": "0.1.0",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": {
            "512": "clickForCode.png"
        }
    },
    "background": {
        "persistent": false,
        "scripts": [
            "clickForCode.js"
        ]
    }
}